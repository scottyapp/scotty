{
  "author": "Martin Wawrusch <martin@wawrusch.com> (http://martinatsunset.com)",
  "name": "scotty",
  "description": "Create your scottyapp.com application backends in minutes.",
  "version": "0.0.1",
  "main" : "lib/index.js",
  "preferGlobal" : true,
  "repository": {
    "type": "git",
    "url": "git://github.com/scottyapp/scotty.git"
  },
  "bin" : {
  },
  "directories" : {
    "lib" : "./lib",
    "test" : "./spec",
    "bin" : "./bin"
  },
  "keywords" : [
    "scottyapp","backend","mobile","api","cloud","cli","platform-as-a-service",
        "deployment","scotty","rest","restful","mongodb"
  ],
  "scripts": {
    "test": "node_modules/.bin/vows --spec spec/*-spec.coffee",
    "watch" : "coffee -c -w  -o lib src"
  },
  "engines": {
    "node": ">=0.4.12"
  },
  "dependencies": {
    "underscore" : "1.2.x",
    "async" : "0.1.x",
    "colors" : "~ 0.5",
    "pkginfo" : "~ 0.2",
    "cliff": "0.1.x",
    "eyes": "0.1.x",
    "optimist": "0.2.x",
    "prompt": "0.1.x >=0.1.7",
    "request": "2.1.x",
    "winston": "0.5.x",
    "nconf" : "0.4.x",
    "scottyapp-api-client" : "0.0.x"
  },
  "devDependencies": {
    "should" : ">=0.2.1",    
    "coffee-script" : ">= 1.1.2",
    "vows" : ">= 0.5.9"
  },
  "bugs": { "url": "http://github.com/scottyapp/scotty/issues" },
  "licenses": [
    { "type": "MIT",
      "url": "http://github.com/scottyapp/scotty/raw/master/LICENSE"
    }
  ]
}
